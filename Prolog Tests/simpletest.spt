module simpletest

language Prolog
start symbol Start

test mixed local and free variables [[
	a(F1, F1, F2) :- b(F1, F2).
	b(1,1).
	
	?- a(G1, G2, G2).
]] run evaluate-full to [_] // true

test bad mixed local and free variables [[
	a(F1, F1, F2) :- b(F1, F2).
	b(1,2).
	
	?- a(G1, G2, G2).
]] run evaluate-full to [] // false